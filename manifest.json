{
  "manifest_version": 3,
  "name": "Second Guesser",
  "version": "1.0",
  "description": "An extension that second-guesses your every move.",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {}
}
